{
    "name": "violetsun/max",
    "description": "Laravel MAX messenger API package",
    "license": "MIT",
    "authors": [
        {
            "name": "demettriss",
            "email": "demettriss@gmail.com",
            "homepage": "https://t.me/demettriss"
        }
    ],
    "homepage": "https://github.com/violetsun/max",
    "keywords": ["Laravel", "MAX"],
    "require": {
        "illuminate/support": "~12"
    },
    "require-dev": {
        "phpunit/phpunit": "~11.0",
        "orchestra/testbench": "~10"
    },
    "autoload": {
        "psr-4": {
            "VioletSun\\MAX\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VioletSun\\MAX\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VioletSun\\MAX\\MAXServiceProvider"
            ],
            "aliases": {
                "MAX": "VioletSun\\MAX\\Facades\\MAX"
            }
        }
    }
}
